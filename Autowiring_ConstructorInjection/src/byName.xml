<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="vehicle-01" class="com.hitachi.autowiring.Vehicle_byName" autowire="byName">

    </bean>

    <bean id="lambo" class="com.hitachi.autowiring.Car">
        <constructor-arg type="int" value="789"/>
        <constructor-arg type="java.lang.String" value="Red"/>
    </bean>

    <bean id="bugati" class="com.hitachi.autowiring.Car">
        <constructor-arg type="int" value="679"/>
        <constructor-arg type="java.lang.String" value="Gold"/>
    </bean>

    <bean id="rolls" class="com.hitachi.autowiring.Car">
        <constructor-arg type="int" value="987"/>
        <constructor-arg type="java.lang.String" value="Silver"/>
    </bean>
</beans>